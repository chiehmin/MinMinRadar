{
  "manifest_version": 2,

  "name": "MinMinRadar",
  "description": "Find and show pokemons in real time",
  "version": "0.1",

  "icons":{
    "128":"icon.png"
  },
  "background":{
    "page": "background.html"
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "notifications",
    "http://*/*",
    "https://*/*"
  ]
}